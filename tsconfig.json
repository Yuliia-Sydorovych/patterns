{
  "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "strict": true,
      "esModuleInterop": true,
      "outDir": "./dist",    // Ensure this points to the correct directory
      "rootDir": "./src",    // Your TypeScript files should be inside the `src` directory
      "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
